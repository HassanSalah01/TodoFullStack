{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "lib": ["ES6", "DOM"],
        "outDir": "./Build",
        "rootDir": "./Src",
        "strict": true,
        "noImplicitAny": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": ["./src"],
    "exclude": ["node_modules", "spec", "build"]
}
